rem config.sys for DOSEMU + FreeDOS
rem note that the initial "D:" is set to "Z:" at the end
SWITCHES=/F
DOS=UMB,HIGH
dosdata=umb
lastdrive=Z
files=40
stacks=0
buffers=10
devicehigh=d:\ems.sys
devicehigh=d:\cdrom.sys
install=d:\lredir.com z: linux\fs\${DOSEMU_LIB_DIR}/commands ro
shellhigh=z:\fdos\bin\command.com /e:1024 /p
